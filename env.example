# Telegram Bot Token
# IMPORTANT: Do NOT put real tokens in this example file.
# Copy this file to ".env" and fill in real values there.
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Admin Telegram User IDs (comma separated)
# Get your ID from @userinfobot
ADMIN_IDS=123456789

# ComfyUI API URL (for image generation)
COMFYUI_API_URL=http://dx.qyxc.vip:18188

# RunPod API Keys (Video generation)
# Set your actual keys only in the local ".env" file.
RUNPOD_API_KEY_I2V=your_runpod_i2v_api_key_here
RUNPOD_API_KEY_T2V=your_runpod_t2v_api_key_here

# Payment Gateway (Optional)
LS_STORE_URL=
LS_VARIANT_ID=
LS_WEBHOOK_SECRET=
CRYPTO_API_KEY=
CRYPTO_MERCHANT_ID=
CRYPTO_WEBHOOK_SECRET=

# Server URL for payment callbacks
SERVER_URL=https://your-domain.com

